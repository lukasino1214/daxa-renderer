{
    "name": "daxa-renderer",
    "version": "0.1.0",
    "dependencies": [
        {
            "name": "daxa",
            "features": [
                "utils-imgui",
                "utils-mem",
                "utils-pipeline-manager-glslang",
                "utils-task-list"
            ]
        },
        {
            "name": "imgui",
            "features": [
                "glfw-binding",
                "docking-experimental"
            ]
        },
        {
            "name": "glfw3",
            "features": [
                "xlib",
                "wayland"
            ]
        },
        "glfw3",
        "glm",
        "tinygltf",
        "entt",
        "yaml-cpp",
        "meshoptimizer",
        "fastgltf"
    ],
    "builtin-baseline": "85c4f11f8d635c408ba63062fd261c14678c0132",
    "overrides": [
        {
            "name": "glfw3",
            "version-string": "custom"
        }
    ]
}